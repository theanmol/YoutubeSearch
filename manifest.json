{
  "manifest_version": 2,

  "name": "SearchYoutube",
  "description": "This extension makes searching for youtube vedios faster",
  "version": "1.0",

  "omnibox": { "keyword" : "y" },
  
  "icons": { "16" : "icon.png"},
  
  "background": {
    "scripts": ["background.js"]
  },
      

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup":"popup.html"
  
  },
  "permissions": [
   "activeTab",
   "tabs",
   "http://suggestqueries.google.com/complete/*"
   ]
}

